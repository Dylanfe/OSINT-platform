{
  "name": "osint-nexus",
  "version": "1.0.0",
  "description": "Advanced OSINT Platform with Comprehensive Tools and API Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
